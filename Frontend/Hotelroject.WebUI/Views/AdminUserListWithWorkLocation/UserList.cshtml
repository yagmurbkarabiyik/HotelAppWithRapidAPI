@model List<Hotelroject.WebUI.Dtos.AppUserDto.ResultAppUserWithWorkLocationDto>
@{
    ViewData["Title"] = "UserList";
    Layout = "~/Views/AdminLayout/_AdminLayouts.cshtml";
}

<table class="table table-xs mb-0">
	<thead>
		<tr>
			<th>#</th>
			<th>Name Surname</th>
			<th>Work Location</th>
			<th>City</th>
			<th>Status</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{

			<tr>
				<td><img src="@item.ImageUrl" class=" rounded-circle mr-3" alt=""></td>
				<td>@item.Name @item.Surname</td>
				<td>@item.WorkLocationName</td>
				<td>@item.City</td>

				<td><i class="fa fa-circle-o text-success  mr-2"></i> Active</td>

			</tr>
		}
</table>
<br />
<a href="/Register/Index/" class="btn btn-primary">Add New User</a>